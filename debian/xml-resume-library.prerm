PACKAGE=xml-resume-library
CENTRALCAT=/etc/sgml/${PACKAGE}.cat

set -e

if [ "$1" = remove ]; then
    update-catalog --quiet --remove --super ${CENTRALCAT}
elif [ "$1" = purge ]; then
    rm -f ${CENTRALCAT} ${CENTRALCAT}.old
    rm -rf /etc/sgml/${PACKAGE}
fi

# automatically generated debhelper commands
#DEBHELPER#

exit 0
